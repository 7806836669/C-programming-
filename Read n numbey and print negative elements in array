#include <stdio.h>
int main()
{
    int arr[MAX_SIZE]; // Declare array of MAX_SIZE
    int i, N;
    printf("Enter size of the array : ");
    scanf("%d", &N);
    printf("Enter elements in array : ");
    for(i=0; i<N; i++)
    {
        scanf("%d", &arr[i]);
    }
    printf("\nAll negative elements in array are : ");
    for(i=0; i<N; i++)
    {
        {
            printf("%d\t", arr[i]);
        }
    }
    return 0;
}
